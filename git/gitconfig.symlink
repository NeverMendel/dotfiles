[include]
        path = ~/.gitconfig.local
[hub]
        protocol = https
[alias]
        amend = commit --amend --no-edit
        st = status
        unstage = reset HEAD --
        last = log -1 HEAD
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[help]
        autocorrect = 1
[push]
        default = simple